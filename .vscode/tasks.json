{
  "version": "2.0.0",
  "tasks": [
    {
      "args": [
        "JEKYLL_ENV=development",
        "bundle",
        "exec",
        "jekyll",
        "serve",
        "--livereload_port",
        "25729",
        "--force_polling",
        "--host",
        "0.0.0.0",
        "--port",
        "4444",
        "--future",
        "--incremental",
        "--livereload",
        "--drafts",
        "--unpublished"
      ],
      "cwd": "${workspaceRoot}/demo",
      "env": {
        "AppData": "/home/mslinn/.config"
      },
      "group": {
        "kind": "build",
        "isDefault": true
      },
      "label": "Start Jekyll Demo Server",
      "presentation": {
        "echo": true,
        "reveal": "always",
        "focus": false,
        "panel": "shared",
        "showReuseMessage": true,
        "clear": false
      },
      "type": "shell"
    },
    {
      "args": [
        "JEKYLL_ENV=development",
        "bundle",
        "exec",
        "jekyll",
        "build",
        "--future"
      ],
      "cwd": "${workspaceRoot}/demo",
      "env": {
        "AppData": "/home/mslinn/.config"
      },
      "group": "build",
      "label": "Build Jekyll Site",
      "presentation": {
        "echo": true,
        "reveal": "always",
        "focus": false,
        "panel": "shared"
      },
      "type": "shell"
    }
  ]
}