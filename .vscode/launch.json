{
  "version": "0.2.0",
  "configurations": [
    {
      "args": [
        "JEKYLL_ENV=development",
        "bundle",
        "exec",
        "jekyll",
        "serve",
        "--livereload_port",
        "25729",
        "--force_polling",
        "--host",
        "0.0.0.0",
        "--port",
        "4444",
        "--future",
        "--incremental",
        "--livereload",
        "--drafts",
        "--unpublished"
      ],
      "cwd": "${workspaceRoot}/demo",
      "env": {
        "AppData": "/home/mslinn/.config"
      },
      "name": "Start Jekyll Server (No Debug)",
      "panel": "shared",
      "presentation": {
        "echo": true,
        "focus": false,
        "reveal": "always"
      },
      "type": "shell"
    },
    {
      "args": [
        "serve",
        "--livereload_port",
        "25729",
        "--force_polling",
        "--host",
        "0.0.0.0",
        "--port",
        "4444",
        "--future",
        "--incremental",
        "--livereload",
        "--drafts",
        "--unpublished"
      ],
      "cwd": "${workspaceRoot}/demo",
      "env": {
        "AppData": "/home/mslinn/.config",
        "JEKYLL_ENV": "development"
      },
      "log": "*",
      "name": "Debug Jekyll Server (With Breakpoints)",
      "navigateStack": true,
      "program": "${workspaceRoot}/binstub/jekyll",
      "request": "launch",
      "showInstructionBreakpoints": false,
      "showReturnValue": true,
      "start": true,
      "stopOnEntry": false,
      "type": "ruby"
    }
  ]
}