{
  "version": "0.2.0",
  "configurations": [
    {
      "args": [
        "exec",
        "jekyll",
        "serve",
        "--drafts",
        "--future",
        "--force_polling",
        "--host",
        "0.0.0.0",
        "--incremental",
        "--livereload",
        "--livereload_port",
        "25729",
        "--port",
        "4444",
        "--unpublished"
      ],
      "cwd": "${workspaceRoot}/demo",
      "env": {
        "AppData": "/home/mslinn/.config",
        "JEKYLL_ENV": "development"
      },
      "name": "Debug Jekyll Demo (Ruby LSP compatible)",
      "program": "${workspaceRoot}/demo",
      "request": "launch",
      "type": "ruby"
    },
    {
      "args": [
        "exec",
        "jekyll",
        "build",
        "--future"
      ],
      "cwd": "${workspaceRoot}/demo",
      "env": {
        "AppData": "/home/mslinn/.config",
        "JEKYLL_ENV": "development"
      },
      "name": "Test Jekyll Build",
      "program": "${workspaceRoot}/demo",
      "request": "launch",
      "type": "ruby"
    }
  ]
}