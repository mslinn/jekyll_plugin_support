{
  "version": "0.2.0",
  "configurations": [
    {
      "args": [
        "JEKYLL_ENV=development",
        "bundle",
        "exec",
        "jekyll",
        "serve",
        "--livereload_port",
        "25729",
        "--force_polling",
        "--host",
        "0.0.0.0",
        "--port",
        "4444",
        "--future",
        "--incremental",
        "--livereload",
        "--drafts",
        "--unpublished"
      ],
      "cwd": "${workspaceRoot}/demo",
      "env": {
        "AppData": "/home/mslinn/.config"
      },
      "name": "Start Jekyll Server",
      "presentation": {
        "echo": true,
        "reveal": "always",
        "focus": false,
        "panel": "shared"
      },
      "type": "shell"
    }
  ]
}